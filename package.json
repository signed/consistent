{
  "name": "@signed/consistent",
  "version": "0.1.0",
  "description": "Basic typescript setup for tdd",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "test": "jest",
    "run:index": "ts-node src/index.ts"
  },
  "devDependencies": {
    "@types/jest": "24.0.11",
    "@types/node": "11.13.6",
    "jest": "24.7.1",
    "ts-jest": "24.0.2",
    "typescript": "3.4.4"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": {
          "warnOnly": true,
          "ignoreCodes": [
            "TS151001"
          ]
        }
      }
    },
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/tests"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/tests/setup.ts"
    ],
    "collectCoverage": true
  },
  "dependencies": {
    "change-case": "3.1.0",
    "ts-node": "8.1.0"
  }
}
